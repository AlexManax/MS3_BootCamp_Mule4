	<munit:test name="API-impl-test-suite-getAllShipsFlowTest" description="Test" doc:id="76e674e3-f7be-464c-8bb6-7f47cba07f68">
		<munit:behavior >
			<munit-tools:mock-when doc:name="Mock when" doc:id="fdab1144-dfe3-4cfc-8571-f07020b41efd" processor="db:select">
				<munit-tools:with-attributes >
					<munit-tools:with-attribute attributeName="doc:name" whereValue="SEL_ALL = Ships" />
				</munit-tools:with-attributes>
				<munit-tools:then-return >
					<munit-tools:payload value="#[[ ['idShip':3, 'nameShip':'Kakashka', 'crewSize':6, 'speed':'10.1'] ]]"/>
				</munit-tools:then-return>
			</munit-tools:mock-when>
			<munit-tools:mock-when doc:name="Mock when" doc:id="204d81bf-394d-4c5b-9401-17e2d4fbf62a" processor="db:select">
				<munit-tools:with-attributes >
					<munit-tools:with-attribute attributeName="doc:name" whereValue="SEL_ALL = Members" />
				</munit-tools:with-attributes>
				<munit-tools:then-return >
					<munit-tools:payload value="#[[ ['rankmember':10.1, 'idship':3, 'idmember':6, 'namemember':'Montgomery Scott'] ]]" />
				</munit-tools:then-return>
			</munit-tools:mock-when>
		</munit:behavior>
		<munit:execution >
			<flow-ref doc:name="Flow-ref to getAllShipsFlow" doc:id="18931671-d0bd-4ffe-b8ef-c9f75a3eb3b2" name="getAllShipsFlow"/>
		</munit:execution>
		<munit:validation>
			<logger level="INFO" doc:name="Logger" doc:id="b1a07112-aef2-4364-8ec6-7baaf373aaaa" message="#[payload]"/>
		</munit:validation>
	</munit:test>
